[flake8]
ignore = C101
exclude =
    **/migrations/*,
    .git,
    .mypy_cache,
    __pycache__,
    venv
filename = *.py
accepted-encodings = utf-8
inline-quotes = double
max-complexity = 12
max-linenumber = 500
max-line-length = 119
multiline-quotes = double
per-file-ignores =
    core/project/settings/templates/settings.dev.py:F821
